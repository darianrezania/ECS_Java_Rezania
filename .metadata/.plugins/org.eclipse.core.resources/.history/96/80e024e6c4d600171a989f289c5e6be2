
public class UsingVariables
{
	public static void main ( String[] args) 
	{	
		int variable_1;
		double variable_2;
		String variable_3;
		
		variable_1 = 113;
		variable_2 = 2.71828;
		variable_3 = Computer_Science; 
		System.out.println("This is room") ;
		System.out.println("e is close to")(variable_2)  );
		System.out.println("I am learning a bit about") (variable_3) ;
		
	}

}
