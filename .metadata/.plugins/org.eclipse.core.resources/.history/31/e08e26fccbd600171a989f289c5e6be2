
public class YourSchedule 
{
	public static void main ( String[] args)
	{
		String myMath, myEnglish,myBiology, myECS, Myers, Farrar, Eddingfield, Mauro;
		myMath= "|Farrar|";
		myBiology= "|Eddingfield|";
		myEnglish="|Myers|";
		Farrar= myMath;
		Mauro=myECS;
		Myers=myEnglish;
		
		
		System.out.println("|Per 1| " + myBiology);
		System.out.println("|Per 2| " +  myECS + Mauro );
		System.out.println("|Per 3| " +  myMath + Farrar);
		System.out.println("|Per 4| " +  myEnglish + Myers);
	}

}
