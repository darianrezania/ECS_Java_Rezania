
public class YourSchedule 
{
	public static void main ( String[] args)
	{
		String myMath, myEnglish,myBiology, myECS, Myers, Farrar, Eddingfield, Mauro;
		myMath=    "|     Farrar       |";
		myBiology= "|     Eddingfield  |";
		myEnglish= "|     Myers        |";
		myECS=     "|     Mauro        |";
		Farrar="|"+ myBiology +"|";
		System.out.println("|Per 1| " + myBiology);
		System.out.println("|Per 2| " +  myECS);
		System.out.println("|Per 3| " +  myMath);
		System.out.println("|Per 4| " +  myEnglish);
	}

}
