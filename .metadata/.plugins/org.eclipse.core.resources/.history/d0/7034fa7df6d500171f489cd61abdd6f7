
public class YourInitials
{
	public static void main(String[] args)
	{System.out.println("DDDDDD      RRRRR");
	 System.out.println("D      D    R     R");
	 System.out.println("D      D    RRRRRRRR");
	 System.out.println("D      D    R      R");
	 System.out.println("D      D    R      R");
	 System.out.println("DDDDDD      R      R");
	}
	
}
